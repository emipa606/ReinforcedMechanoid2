<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <!-- ================================ ARC Weapons ====================== -->
  <SoundDef>
    <defName>RM_Interact_ARC</defName>
    <context>MapOnly</context>
    <maxSimultaneous>1</maxSimultaneous>
    <subSounds>
      <li>
        <grains>
          <li Class="AudioGrain_Clip">
            <clipPath>UI/WeaponHandling/HandleWeapon_ARC</clipPath>
          </li>
        </grains>
      </li>
    </subSounds>
  </SoundDef>
  <SoundDef>
    <defName>RM_Shot_ARC</defName>
    <context>MapOnly</context>
    <maxSimultaneous>1</maxSimultaneous>
    <subSounds>
      <li>
        <grains>
          <li Class="AudioGrain_Folder">
            <clipFolderPath>Weapon/ARC_Shot</clipFolderPath>
          </li>
        </grains>
      </li>
    </subSounds>
  </SoundDef>
