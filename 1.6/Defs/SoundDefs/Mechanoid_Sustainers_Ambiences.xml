<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <SoundDef>
    <defName>RM_Wraith_Ambience_Start</defName>
    <context>MapOnly</context>
    <subSounds>
      <li>
        <grains>
          <li Class="AudioGrain_Clip">
            <clipPath>Misc/Wraith/Wraith_Ambience_Start</clipPath>
          </li>
        </grains>
        <volumeRange>16~16</volumeRange>
        <distRange>0~50.27907</distRange>
        <sustainLoop>False</sustainLoop>
      </li>
    </subSounds>
  </SoundDef>
  <SoundDef>
    <defName>RM_Wraith_Ambience_Stop</defName>
    <context>MapOnly</context>
    <subSounds>
      <li>
        <grains>
          <li Class="AudioGrain_Clip">
            <clipPath>Misc/Wraith/Wraith_Ambience_Out</clipPath>
          </li>
        </grains>
        <volumeRange>16~16</volumeRange>
        <distRange>0~50.27907</distRange>
        <sustainLoop>False</sustainLoop>
      </li>
    </subSounds>
  </SoundDef>
  <SoundDef>
    <defName>RM_Wraith_Ambience</defName>
    <sustain>True</sustain>
    <context>MapOnly</context>
    <priorityMode>PrioritizeNearest</priorityMode>
    <sustainStartSound>RM_Wraith_Ambience_Start</sustainStartSound>
    <sustainStopSound>RM_Wraith_Ambience_Stop</sustainStopSound>
    <subSounds>
      <li>
        <muteWhenPaused>True</muteWhenPaused>
        <grains>
          <li Class="AudioGrain_Clip">
            <clipPath>Misc/Wraith/Wraith_Ambience_Loop</clipPath>
          </li>
        </grains>
        <volumeRange>15~15</volumeRange>
        <pitchRange>1~1.015</pitchRange>
        <distRange>0~30</distRange>
      </li>
    </subSounds>
  </SoundDef>
  <SoundDef>
    <defName>RM_MechanoidCrate_Ambience</defName>
    <sustain>True</sustain>
    <context>MapOnly</context>
    <priorityMode>PrioritizeNearest</priorityMode>
    <subSounds>
      <li>
        <grains>
          <li Class="AudioGrain_Folder">
            <clipPath>Misc/MechanoidCrate</clipPath>
          </li>
        </grains>
        <volumeRange>20~30</volumeRange>
        <distRange>15~25</distRange>
      </li>
    </subSounds>
  </SoundDef>
</Defs>
