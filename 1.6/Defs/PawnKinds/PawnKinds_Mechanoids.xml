      <li MayRequire="Ludeon.Rimworld.Biotech">
        <bodyGraphicData>
          <texPath>Things/Pawn/Mechanoid/Mech_Vulture_New/Mech_VultureClean</texPath>
          <maskPath>Things/Pawn/Mechanoid/AllegianceOverlays/Mech_Vulture</maskPath>
          <shaderType>CutoutWithOverlay</shaderType>
          <graphicClass>Graphic_Multi</graphicClass>
          <drawSize>1.0</drawSize>
          <shadowData>
            <volume>(0.4, 0.8, 0.4)</volume>
          </shadowData>
        </bodyGraphicData>
