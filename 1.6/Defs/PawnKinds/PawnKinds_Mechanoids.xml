  <PawnKindDef ParentName="BaseMechanoidKind" Name="RM_Pawn_Vulture">
    <defName>RM_Mech_Vulture</defName>
    <label>vulture</label>
    <race>RM_Mech_Vulture</race>
    <combatPower>50</combatPower>
    <isFighter>false</isFighter>
    <controlGroupPortraitZoom MayRequire="Ludeon.Rimworld.Biotech">1.8</controlGroupPortraitZoom>
